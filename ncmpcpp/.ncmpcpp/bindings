def_key "h"
	previous_column

def_key "h"
	volume_down

def_key "j"
	scroll_down

def_key "k"
	scroll_up

def_key "l"
	next_column

def_key "l"
	volume_up

def_key ";"
	show_lyrics

def_key "g"
	move_home

def_key "G"
	move_end

def_key "ctrl-f"
	page_down

def_key "ctrl-b"
	page_up

def_key "d"
	delete_playlist_items

def_key "d"
	delete_browser_items

def_key "d"
	delete_stored_playlist

def_key "J"
	select_item
	scroll_down

def_key "K"
	select_item
	scroll_up

def_key "s"
	select_item
